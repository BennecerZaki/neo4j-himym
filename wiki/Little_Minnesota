        '</scr' + 'ipt><ins class="adsbygoogle ad-responsive-ic" style="display:inline-block;" ' +
        'data-ad-client="ca-pub-4086838842346968" data-ad-channel="mon_pl_bc_bt_ic_fsm_ffv mon_hub_tv mon_sl_ic" ' +
        'data-ad-type="text" ' + 
