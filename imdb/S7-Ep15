                    <button class="btn small" value="Yes" name="ynb_2583534_yes" onclick="CS.TMD.user_review_vote(2583534, 'tt2216757', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_2583534_no" onclick="CS.TMD.user_review_vote(2583534, 'tt2216757', 'no');" >No</button>
                </div>
